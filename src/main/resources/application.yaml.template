server:
  port: ?
spring:
  jpa:
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5InnoDBDialect
        temp:
          use_jdbc_metadata_defaults: false
        hbm2ddl:
          auto: validate
    database-platform: org.hibernate.dialect.MySQL8Dialect
  datasource:
    url: ?
    driver-class-name: com.mysql.cj.jdbc.Driver
    username: ?
    password: ?
